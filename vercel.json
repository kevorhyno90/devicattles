{
  "version": 2,
  "engines": {
    "node": "20.x"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "node -v && npm -v && npm run build",
        "outputDirectory": "dist"
      }
    }
  ]
}

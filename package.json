{
  "name": "devins-farm",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5000 --strictPort",
    "clean": "rm -rf dist",
    "build": "npm run clean && vite build",
    "preview": "vite preview",

    "deploy:firebase": "npm run build && firebase deploy --only hosting"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "docx": "^9.5.1",
    "firebase": "^12.6.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.0",
    "gh-pages": "^6.3.0",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0"
  }
}
